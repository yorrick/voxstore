# Sentry DSN for error tracking
SENTRY_DSN=

# Server port
BACKEND_PORT=8000

# Frontend origin for CORS (when running separately)
FRONTEND_PORT=5173

# CORS allowed origins for production (comma-separated)
# Example: ALLOWED_ORIGINS=https://voxstore.onrender.com,https://www.voxstore.com
# Localhost origins are always allowed for local development
ALLOWED_ORIGINS=

# ElevenLabs API key for voice transcription (Scribe v2)
# Get yours at https://elevenlabs.io
ELEVENLABS_API_KEY=

# OpenRouter API key for semantic search embeddings
# Get yours at https://openrouter.ai/keys
OPENROUTER_API_KEY=
